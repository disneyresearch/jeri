{
  "name": "jeri",
  "version": "2.0.0",
  "license": "Modified Apache 2.0 License (see the license file)",
  "homepage": "https://jeri.io",
  "keywords": [
    "EXR",
    "OpenEXR",
    "HDR",
    "image",
    "viewer"
  ],
  "description": "JavaScript Extended-Range Image viewer",
  "dependencies": {
    "common-prefix": "1.1.0",
    "fast-levenshtein": "^3.0.0",
    "lodash": "^4.17.20",
    "memoize-one": "^5.1.1",
    "raw-loader": "^4.0.1"
  },
  "devDependencies": {
    "@types/fast-levenshtein": "0.0.1",
    "@types/lodash": "^4.14.168",
    "@types/memoize-one": "^4.1.1",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "4.1.8",
    "file-loader": "^6.2.0",
    "json-loader": "0.5.7",
    "serve": "^11.3.2",
    "source-map-loader": "0.2.4",
    "tslint": "5.16.0",
    "tslint-react": "4.0.0",
    "typedoc": "^0.15.0",
    "typescript": "^4.2.3",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "worker-loader": "^3.0.8"
  },
  "peerDependencies": {
    "normalize-wheel": "1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "5.2.1"
  },
  "scripts": {
    "install": "npm run build-npm",
    "build": "npm run build-npm && npm run build-web",
    "build-npm": "tsc && cp -r ./src/exr-wrap ./src/shaders build_npm && cp -r ./src/utils/exr-parser.worker.js build_npm/utils",
    "build-npm-test": "tsc && tsc src/exr-parser.worker.ts --skipLibCheck && cp -r ./src/exr-wrap ./src/shaders build_npm && cp -r ./src/utils/exr-parser.worker.js build_npm/utils",
    "build-web": "webpack",
    "develop": "npm run build-npm && webpack -d --mode=development",
    "clean": "rm -rf build build_web/jeri* build_web/*.js build_web/*.wasm build_web/*.map build_npm",
    "docs": "typedoc --out documentation src",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "examples": "serve build_web"
  },
  "main": "build_npm/jeri.js",
  "types": "build_npm/jeri.d.ts"
}

